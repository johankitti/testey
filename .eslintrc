{
  "extends": ["prettier"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module",
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "env": {
    "jest": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "quotes": ["error", "single"],
    "curly": ["error", "all"]
  }
}
